package twicons

templ Anchor(class ...string) {
	<svg
		xmlns="http://www.w3.org/2000/svg"
		width="24"
		height="24"
		viewBox="0 0 24 24"
		fill="none"
		stroke="currentColor"
		stroke-width="2"
		stroke-linecap="round"
		stroke-linejoin="round"
		class={ class }
	>
		<path d="M12 22V8"></path>
		<path d="M5 12H2a10 10 0 0 0 20 0h-3"></path>
		<circle cx="12" cy="5" r="3"></circle>
	</svg>
}
